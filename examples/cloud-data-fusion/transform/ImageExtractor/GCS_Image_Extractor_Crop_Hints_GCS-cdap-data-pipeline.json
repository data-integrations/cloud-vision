                    "name": "ImageExtractor",
                    "type": "transform",
                    "label": "Image Extractor",
                    "artifact": {
                        "name": "cloud-vision",
                        "version": "1.0.0-SNAPSHOT",
                        "scope": "USER"
                    },
                    "properties": {
                        "serviceFilePath": "auto-detect",
                        "project": "auto-detect",
